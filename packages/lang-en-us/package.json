  "dependencies": {
    "@sharpee/core": "workspace:*",
    "@sharpee/if-domain": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^18.15.11",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "eslint": "^8.38.0",
    "rimraf": "^4.4.1",
    "typescript": "^5.0.4"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "interactive-fiction",
    "if",
    "text-adventure",
    "sharpee",
    "language",
    "english",
    "i18n"
  ],
  "author": "Sharpee Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ChicagoDave/sharpee.git",
    "directory": "packages/lang-en-us"
  },
  "homepage": "https://github.com/ChicagoDave/sharpee#readme",
  "bugs": {
    "url": "https://github.com/ChicagoDave/sharpee/issues"
